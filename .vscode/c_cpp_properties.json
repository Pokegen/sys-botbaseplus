{
	"configurations": [
		{
			"name": "Linux",
			"includePath": [
				"${env:DEVKITPRO}/libnx/include/",
				"${env:DEVKITPRO}/devkitA64/include/",
				"${env:DEVKITPRO}/devkitA64/aarch64-none-elf/include/",
				"${env:DEVKITPRO}/devkitARM/include/",
				"${env:DEVKITPRO}/devkitA64/lib/gcc/aarch64-none-elf/10.1.0/include/",
				"${workspaceFolder}/sys-botbaseplus/include/**",
				"/usr/local/include/",
				"/usr/include/linux/"
			],
			"defines": [
				"SWITCH",
				"__SWITCH__",
				"DEBUG"
			],
			"compilerPath": "/usr/bin/gcc-10",
			"cStandard": "c11",
			"cppStandard": "c++20",
			"intelliSenseMode": "gcc-x64"
		},
		{
			"name": "MacOs",
			"browse": {
				"path": [
					"${workspaceRoot}"
				],
				"limitSymbolsToIncludedHeaders": true
			},
			"includePath": [
				"${env:DEVKITPRO}/libnx/include/",
				"${env:DEVKITPRO}/devkitA64/include/",
				"${env:DEVKITPRO}/devkitA64/aarch64-none-elf/include/",
				"${env:DEVKITPRO}/devkitARM/include/",
				"${workspaceFolder}/sys-botbaseplus/include/**",
				"/opt/homebrew/Cellar/gcc/11.2.0_3/include/c++/11"
			],
			"defines": [
				"SWITCH",
				"__SWITCH__",
				"DEBUG"
			],
			"compilerPath": "/opt/homebrew/Cellar/gcc/11.2.0_3/bin/gcc-11",
			"cStandard": "c11",
			"cppStandard": "c++20",
			"intelliSenseMode": "clang-arm64"
		}
	],
	"version": 4
}
